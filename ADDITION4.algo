ALGORITHM addition
VAR
    tab1: ARRAY_OF_INTEGER[n];
    tab2: ARRAY_OF_INTEGER[m];
    i,j:INTEGER;
    somme: INTEGER:=0;
BEGIN
    FOR i FROM 0 TO n-1 STEP 1 DO
    
    FOR j FROM 0 TO m-1 STEP 1 DO
    IF (tab1[i] == tab2[j]) THEN
        somme:= somme+((tab1[i])*2);
    END_IF
    END_FOR
END